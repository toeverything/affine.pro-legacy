{
  "name": "affine.pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node script/start-dev.js",
    "dev": "npm run start",
    "build": "node script/pre-build.js && next build && next export && npm run build:optimage",
    "build:optimage": "next-image-export-optimizer && mv out/nextImageExportOptimizer/* out/_next/static/media/ && rm -rf out/nextImageExportOptimizer",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "typeCheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.10",
    "@mui/system": "^5.10.10",
    "@next/mdx": "^12.3.1",
    "chokidar": "^3.5.3",
    "copy-dir": "^1.3.0",
    "gray-matter": "^4.0.3",
    "i18next": "^21.10.0",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "react-infinite-scroll-component": "^6.1.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.3.1",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-image-export-optimizer": "^0.17.1",
    "next-transpile-modules": "^9.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16",
    "npm": "please-use-pnpm",
    "pnpm": ">=7",
    "yarn": "please-use-pnpm"
  }
}
